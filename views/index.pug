html
	include includes/head.pug
	body
		.container
			.row
				h1.display-4.m-b-2 People that have signed up so far!
				ul.user_list
					each user in users
						li.user_item #{user.firstname} #{user.lastname}
				div.col-md-3
				a(href="signup")
					button.btn.btn-default Sign-up!
				a(href="search")
						button.btn.btn-default Search!
		
